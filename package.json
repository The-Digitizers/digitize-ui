{
  "name": "digitize-ui",
  "version": "0.0.0",
  "private": true,
  "description": "Ui library",
  "workspaces": [
    "storybook/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "dev": "turbo run dev",
    "clean": "turbo run clean && rm -rf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=theme... && changeset publish"
  },
  "author": "Trent Ungard",
  "license": "ISC",
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.24.3",
    "@types/react": "^18",
    "react": "^18.2.0",
    "tsup": "^6.2.2",
    "turbo": "^1.4.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.2.2"
}
